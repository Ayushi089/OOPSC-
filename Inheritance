// Inheritance Example 

#include<bits/stdc++.h>
using namespace std;
class Parent {
    public :
    int p;
};
class child : public Parent {
    public :
    int c;
};

int main(){
    child obj1;
    obj1.c=7;
    obj1.p=9;
    cout<<"Child is "<<obj1.c<<endl;
    cout<<"Parent is "<<obj1.p;
}



